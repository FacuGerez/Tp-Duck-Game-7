    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debuguer server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/server",
            "args": [
                "8080"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Habilitar la impresión con sangría para gdb",
