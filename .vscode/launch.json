                    "ignoreFailures": true
                },
                {
                    "description": "Establecer tipo de desensamblado en Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "visualizerFile": "/home/cpiccin/.config/Code/User/workspaceStorage/ab8a7a61b5fae425b5de12c4e6955de5/tonka3000.qtvsctools/qt.natvis.xml"
